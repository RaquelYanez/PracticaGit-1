package EjemploManuel;

public class Rectangulo extends Figura{
	//definir atrbutos
	private float base;
	private float altura;
	
	//constructor publico sino no podriamos crear objetos desde otra clase
	
	public Rectangulo (float base, float altura) { //float espera los aprametros float
this.base = base;
this.altura = altura;
}
	public double obtenerArea() {
		return base*altura;
	}
}